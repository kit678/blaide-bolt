{
  "version": 2,
  "builds": [
    {
      "src": "src/api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/$1"
    },
    {
      "src": "/sw.js",
      "dest": "dist/sw.js"
    },
    {
      "src": "/workbox-(.*).js",
      "dest": "dist/workbox-$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 